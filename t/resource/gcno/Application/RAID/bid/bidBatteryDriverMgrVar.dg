
bidBatteryDriverMgrVar.cc: $(wildcard .bidBatteryDriverMgrVar.cc/bidBatteryDriverMgrVar.cc .bidBatteryDriverMgrVar.cc/bigleverfilelogic.bllt)
	if [ ! -d $(CURDIR)/.bidBatteryDriverMgrVar.cc ]; then \
	    echo "invalid variation point: $(CURDIR)/.bidBatteryDriverMgrVar.cc"; \
